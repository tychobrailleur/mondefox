{
  "manifest_version": 2,
  "name": "mondefox",
  "version": "0.4",
  "license": "GPLv3",
  "description": "Customise the layout of lemonde.fr",
  "homepage_url": "https://github.com/tychobrailleur/mondefox",
  "icons": {
    "64": "icons/social-64.png"
  },

  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.lemonde.fr/*"],
      "js": ["data/cleanup.js"]
    }
  ]
}
